{
  "name": "tech-challenge-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "node --watch --env-file-if-exists .env server.js",
    "start": "node --env-file-if-exists .env server.js",
    "test": "dotenv -e .env -- jest --passWithNoTests",
    "test:ci": "dotenv -e .env -- jest --ci --passWithNoTests",
    "test:coverage": "dotenv -e .env -- jest --coverage --passWithNoTests",
    "test:watch": "dotenv -e .env -- jest --watch --passWithNoTests",
    "test:watchAll": "dotenv -e .env -- jest --watchAll --passWithNoTests",
    "db:migrate": "dotenv -e .env -- sequelize db:migrate --config ./src/database/config.js --migrations-path ./src/database/migrations",
    "migration:create": "sequelize migration:create --migrations-path ./src/database/migrations --name $1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "sequelize": "^6.35.1",
    "zod": "^4.0.9"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.0.5",
    "@swc/core": "^1.13.3",
    "@swc/jest": "^0.2.39",
    "dotenv-cli": "^9.0.0",
    "jest": "^30.0.5",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4"
  }
}
